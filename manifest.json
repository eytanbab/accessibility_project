{
  "name": "Accessibility Toolbar",
  "description": "A toolbar for people with visual impairment",
  "version": "1.0",
  "manifest_version": 2,
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "permissions": ["activeTab"],
  "browser_action": {
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    },
    "default_title": "Accessibility Toolbar",
    "default_popup": "popup.html"
  }
}
